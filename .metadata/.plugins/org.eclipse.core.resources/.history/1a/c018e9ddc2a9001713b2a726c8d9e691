package V1;

import static org.junit.Assert.*;

import org.junit.Test;

import utiles.Util;

public class SeresTest {

	@Test
	public void test() {
		Fuente instancia=new Fuente(10);
		instancia.start();
		Util.pausita(100);
		Seres uno=new Seres(instancia, "Tiburcio", 12);
		uno.start();
		assertEquals(0, uno.getTamano());
	}

}
